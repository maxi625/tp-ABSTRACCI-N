class figura:
    def calcular_area(self):
      pass
  
  
class circulo(figura):
    def calcular_area(self):
     return "Área del círculo = 28.27 unidades² (aproximadamente)"
        
class rectangulo(figura):
    def calcular_area(self):
     return "Área del rectángulo = 20 unidades² "

        
e1=circulo()
print(e1.calcular_area())

e2=rectangulo()
print(e2.calcular_area())
